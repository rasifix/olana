<td width="20">{{#if runner.valid}}{{view Ember.Checkbox checkedBinding="runner.checked"}}{{/if}}</td>
<td width="20" align="right">{{#if runner.rank}}{{runner.rank}}.{{/if}}</td>
<td>{{#link-to runnerRoute runner.id}}{{runner.fullName}}{{/link-to}}</td>
<td>{{runner.yearOfBirth}}</td>
<td>{{runner.city}}</td>
<td>{{runner.club}}</td>
<td>{{runner.time}}</td>
{{#if runner.category}}<td>{{#link-to 'category' runner.category}}{{runner.category}}{{/link-to}}</td>{{/if}}
